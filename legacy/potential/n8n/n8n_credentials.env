# =============================================================================
# STORAGE & MEDIA CONFIGURATION
# =============================================================================
N8N_STORAGE_PATH=/app/data/n8n_storage
N8N_TEMP_DIR=/tmp/n8n_temp
N8N_MAX_FILE_SIZE=100MB

# Cloud Storage (Optional - for production)
# AWS_S3_BUCKET=livhana-media
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
# AWS_REGION=us-east-1

# =============================================================================
# MONITORING & LOGGING
# =============================================================================
N8N_LOG_LEVEL=info
N8N_LOG_OUTPUT=console,file
N8N_LOG_FILE_MAXSIZE=50MB
N8N_LOG_FILE_MAXFILES=5

# Health Check Configuration
N8N_HEALTH_CHECK_ACTIVE=true
N8N_HEALTH_CHECK_PORT=5679

# =============================================================================
# PERFORMANCE TUNING
# =============================================================================
N8N_MAX_CONCURRENT_WORKFLOWS=10
N8N_MAX_EXECUTIONS=100
N8N_EXECUTION_TIMEOUT=300000  # 5 minutes
N8N_WEBHOOK_TIMEOUT=30000     # 30 seconds

# Memory and CPU limits
N8N_MEMORY_MAX=2GB
N8N_CPU_CORES=2

# =============================================================================
# DEVELOPMENT SETTINGS
# =============================================================================
N8N_ENV=production
N8N_DISABLE_UI=false
N8N_DIAGNOSTICS_ENABLED=false

# =============================================================================
# CUSTOM WORKFLOW VARIABLES
# =============================================================================
LIVHANA_VIDEO_PIPELINE_ACTIVE=true
LIVHANA_MUSIC_GENERATION_ACTIVE=true
LIVHANA_VOICE_SYNTHESIS_ACTIVE=true
LIVHANA_CARTOON_ANIMATION_ACTIVE=true
LIVHANA_DISTRIBUTION_ACTIVE=true

# Quality presets
LIVHANA_DEFAULT_VIDEO_QUALITY=high
LIVHANA_DEFAULT_MUSIC_DURATION=180
LIVHANA_DEFAULT_VOICE_MODEL=eleven_monolingual_v1

