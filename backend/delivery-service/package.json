{
  "name": "@livhana/delivery-service",
  "version": "2.0.0",
  "description": "Multi-carrier delivery orchestration - Unfuckwithable Code Protocol (Skipcart, Roadie, Favor, Dispatch, Senpex)",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc && esbuild src/index.ts --bundle --platform=node --external:express --outfile=dist/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit",
    "test:properties": "NODE_OPTIONS=--experimental-vm-modules jest tests/properties",
    "test:fuzz": "NODE_OPTIONS=--experimental-vm-modules jest tests/fuzz --maxWorkers=4",
    "test:all": "npm run test:unit && npm run test:properties && npm run test:fuzz",
    "lint": "eslint src tests --ext .ts --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "ci": "npm run format:check && npm run lint && npm run typecheck && npm run test:all"
  },
  "keywords": [
    "delivery",
    "lightspeed",
    "skipcart",
    "roadie",
    "favor",
    "dispatch",
    "senpex",
    "cannabis"
  ],
  "author": "Jesse Niesen",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "zod": "^3.22.0",
    "@google-cloud/pubsub": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/cors": "^2.8.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.55.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
