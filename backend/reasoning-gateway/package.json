{
  "name": "reasoning-gateway",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "node --watch src/index.js",
    "start": "node src/index.js",
    "test": "vitest",
    "test:autonomous": "bash test-autonomous-api.sh",
    "autonomous:monitor": "node example-sse-client.js",
    "improvement:daily": "node scripts/run-improvement-cycle.js --type=daily",
    "improvement:weekly": "node scripts/run-improvement-cycle.js --type=weekly",
    "improvement:monthly": "node scripts/run-improvement-cycle.js --type=monthly",
    "improvement:full": "node scripts/run-improvement-cycle.js --type=full",
    "improvement:auto": "node scripts/run-improvement-cycle.js --type=daily --auto-execute",
    "improvement:dry-run": "node scripts/run-improvement-cycle.js --type=daily --dry-run",
    "setup:cron": "bash scripts/setup-cron.sh"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@google-cloud/bigquery": "^7.9.4",
    "@sentry/node": "^10.17.0",
    "@sentry/profiling-node": "^10.17.0",
    "backend-common": "file:../common",
    "bullmq": "^5.12.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "newrelic": "^13.4.0",
    "openai": "^4.67.3",
    "pino": "^9.3.2",
    "rate-limit-redis": "^4.2.2",
    "redis": "^4.7.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "vitest": "^3.2.4"
  },
  "lastOptimized": "2025-10-02"
}
